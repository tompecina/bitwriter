<?xml version="1.0" encoding="UTF-8"?>
<file xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.pecina.cz/xsd/bin-1.0.xsd" version="1.0">
  <stream>
    <reset name="i"/>
    <hex repeat="0x100">
      <parity name="pe1" model="even" type="stream-in"/>
      <parity name="po1" model="odd" type="stream-in"/>
      <parity name="pe2" model="even" type="aggregate-stream-in"/>
      <parity name="po2" model="odd" type="aggregate-stream-in"/>
      <parity name="pe3" model="even" type="bitstream"/>
      <parity name="po3" model="odd" type="bitstream"/>
      <parity name="pe4" model="even" type="aggregate-stream-out"/>
      <parity name="po4" model="odd" type="aggregate-stream-out"/>
      <parity name="pe5" model="even" type="stream-out"/>
      <parity name="po5" model="odd" type="stream-out"/>
      <parity name="pe6" model="even" type="output-stream"/>
      <parity name="po6" model="odd" type="output-stream"/>
      {{i++}}
      <release name="pe1"/>
      <release name="po1"/>
      <release name="pe2"/>
      <release name="po2"/>
      <release name="pe3"/>
      <release name="po3"/>
      <release name="pe4"/>
      <release name="po4"/>
      <release name="pe5"/>
      <release name="po5"/>
      <release name="pe6"/>
      <release name="po6"/>
      <put name="pe1"/>
      <put name="po1"/>
      <put name="pe2"/>
      <put name="po2"/>
      <put name="pe3"/>
      <put name="po3"/>
      <put name="pe4"/>
      <put name="po4"/>
      <put name="pe5"/>
      <put name="po5"/>
      <put name="pe6"/>
      <put name="po6"/>
      ee ee ee
    </hex>
  </stream>
  <stream reflect-in="true">
    <reset name="i"/>
    <hex repeat="0x100">
      <parity name="pe1" model="even" type="stream-in"/>
      <parity name="po1" model="odd" type="stream-in"/>
      <parity name="pe2" model="even" type="aggregate-stream-in"/>
      <parity name="po2" model="odd" type="aggregate-stream-in"/>
      <parity name="pe3" model="even" type="bitstream"/>
      <parity name="po3" model="odd" type="bitstream"/>
      <parity name="pe4" model="even" type="aggregate-stream-out"/>
      <parity name="po4" model="odd" type="aggregate-stream-out"/>
      <parity name="pe5" model="even" type="stream-out"/>
      <parity name="po5" model="odd" type="stream-out"/>
      <parity name="pe6" model="even" type="output-stream"/>
      <parity name="po6" model="odd" type="output-stream"/>
      {{i++}}
      <release name="pe1"/>
      <release name="po1"/>
      <release name="pe2"/>
      <release name="po2"/>
      <release name="pe3"/>
      <release name="po3"/>
      <release name="pe4"/>
      <release name="po4"/>
      <release name="pe5"/>
      <release name="po5"/>
      <release name="pe6"/>
      <release name="po6"/>
      <put name="pe1"/>
      <put name="po1"/>
      <put name="pe2"/>
      <put name="po2"/>
      <put name="pe3"/>
      <put name="po3"/>
      <put name="pe4"/>
      <put name="po4"/>
      <put name="pe5"/>
      <put name="po5"/>
      <put name="pe6"/>
      <put name="po6"/>
      ee ee ee
    </hex>
  </stream>
</file>
